      "./src/client/bower_components/spin.js/spin.min.js",
      "./src/client/bower_components/angular-spinner/angular-spinner.min.js"
    ],
    "vendorcss": [
      "./src/client/bower_components/bootstrap/dist/css/bootstrap.css",
      "./src/client/bower_components/angular-print/angularPrint.css"
    ],
    "css": [
      "./src/client/css/app.css",
      "./src/client/css/animations.css"
    ],
    "js_core": [
      "./src/client/app/*.js",
      "./src/client/app/core/*.js"
    ],
    "app_js": [
      "./src/client/app/app.env.config.js",
      "./src/client/app/app.module.js",
      "./src/client/app/app.config.routing.js",
      "./src/client/app/core/core.module.js",
      "./src/client/app/core/constants.js",
      "./src/client/app/core/search-res-parser.js",
      "./src/client/app/core/search-service.js",
      "./src/client/app/core/data-service.js",
      "./src/client/app/core/saved-records-service.js",
      "./src/client/app/core/storage-service.js",
      "./src/client/app/widgets/widgets.module.js",
      "./src/client/app/widgets/config.js",
      "./src/client/app/widgets/scroll.js",
      "./src/client/app/widgets/animations.js",
      "./src/client/app/widgets/utils.js",
      "./src/client/app/home/home.module.js",
      "./src/client/app/home/home.controller.js",
      "./src/client/app/search/search.module.js",
